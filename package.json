{
  "name": "martins-giberts.github.io",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/martins-giberts/martins-giberts.github.io.git",
  "author": "Martins Giberts <martins.giberts@gmail.com>",
  "license": "GPL-3.0",
  "dependencies": {
    "prop-types": "15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "styled-components": "^3.1.5"
  },
  "devDependencies": {
    "autoprefixer": "6.7.7",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "19.0.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-dynamic-import-node": "1.0.1",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "enzyme": "2.8.2",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^3.19.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.3",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "2.28.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "19.0.2",
    "jest-cli": "19.0.2",
    "path": "0.12.7",
    "react-addons-test-utils": "^15.5.1",
    "react-hot-loader": "3.0.0-beta.6",
    "react-test-renderer": "^15.5.4",
    "style-loader": "^0.16.1",
    "uglify-js": "^2.8.22",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "webpack": "^2.4.1",
    "webpack-cleanup-plugin": "0.5.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-merge": "4.1.0",
    "webpack2-validator": "1.0.0"
  },
  "scripts": {
    "build:dev": "webpack --env=dev --progress --profile --colors",
    "build:prod": "yarn test:all:prod && webpack --env=prod --progress --profile --colors",
    "coverage": "jest --coverage --config ./jest.json",
    "eslint": "./node_modules/eslint/bin/eslint.js ./app",
    "start:prod": "yarn build:prod && webpack-dev-server --env=prod",
    "start:dev:docker": "yarn && yarn build:dev && webpack-dev-server --host 0.0.0.0 --env=dev --hot --inline  --colors",
    "start": "yarn && webpack-dev-server --env=dev --hot --inline  --colors",
    "test:all:dev": "yarn validate-webpack:dev && yarn eslint && jest --config ./jest.json",
    "test:all:prod": "yarn validate-webpack:prod && yarn eslint && jest --config ./jest.json",
    "test": "jest -o --config ./jest.json",
    "validate-webpack:dev": "./node_modules/webpack2-validator/dist/bin/webpack-validator.js webpack.config.js --env=dev",
    "validate-webpack:prod": "./node_modules/webpack2-validator/dist/bin/webpack-validator.js webpack.config.js --env=prod"
  },
  "jest": {
    "modulePaths": [
      "/app"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css)$": "identity-obj-proxy"
    },
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/enzyme/"
    ]
  }
}
